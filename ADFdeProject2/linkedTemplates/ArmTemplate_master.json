{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFdeProject2"
		},
		"ds_json_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_json_data'"
		},
		"airQualityAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.waqi.info/feed"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFdeProject2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_json_data_connectionString": {
						"value": "[parameters('ds_json_data_connectionString')]"
					},
					"airQualityAPI_properties_typeProperties_url": {
						"value": "[parameters('airQualityAPI_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}